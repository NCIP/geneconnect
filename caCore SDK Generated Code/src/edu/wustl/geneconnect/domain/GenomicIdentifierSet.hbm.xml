<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="edu.wustl.geneconnect.domain">
  <class name="GenomicIdentifierSet" table="GENOMIC_IDENTIFIER_SET" lazy="true" polymorphism="explicit">
    <cache usage="read-write" />
    <id name="id" type="java.lang.Long" column="SET_ID">
      <generator class="assigned" />
    </id>
    <set name="orderOfNodeTraversalCollection" table="SET_ONT" lazy="true">
      <cache usage="read-write" />
      <key column="SET_ID" />
      <many-to-many class="edu.wustl.geneconnect.domain.OrderOfNodeTraversal" column="PATH_ID" />
    </set>
	<property name="confidenceScore">
	<formula>
	(select 1 from dual)
	</formula>
	</property>
    <many-to-one name="protein" class="edu.wustl.geneconnect.domain.Protein" column="PROTEIN_ID" lazy="true" />
    <many-to-one name="messengerRNA" class="edu.wustl.geneconnect.domain.MessengerRNA" column="MRNA_ID" lazy="true" />
    <set name="consensusIdentifierDataCollection" lazy="true" inverse="true" >
	  <cache usage="read-write" />
	  <key property-ref="confidenceScore" />
      <one-to-many class="edu.wustl.geneconnect.domain.ConsensusIdentifierData" />
    </set>
    <many-to-one name="gene" class="edu.wustl.geneconnect.domain.Gene" column="GENE_ID" lazy="true" />
    <!--many-to-one name="genomicIdentifierSolution" class="edu.wustl.geneconnect.GenomicIdentifierSolution" column="SOLUTION_ID" lazy="true" /-->
  </class>
</hibernate-mapping>

