<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="edu.wustl.geneconnect.domain.impl">
  <class name="LinkTypeImpl" table="LINKTYPE" lazy="true">
    <cache usage="read-write" />
    <id name="id" type="java.lang.Long" column="LINK_TYPE_ID">
      <generator class="assigned" />
    </id>
    <property name="type" type="java.lang.String" column="LINK_TYPE_NAME" />
  </class>
</hibernate-mapping>

