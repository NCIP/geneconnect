<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="edu.wustl.geneconnect.impl">
  <class name="GenomicIdentifierSolutionImpl" table="SOLUTION" lazy="true" polymorphism="explicit">
    <cache usage="read-write" />
	<id name="id" type="java.lang.Long" column="SOLUTION_ID">
      <generator class="assigned" />
    </id>
    <set name="genomicIdentifierSetCollection" lazy="true" inverse="true">
      <cache usage="read-write" />
      <key column="SOLUTION_ID" />
      <one-to-many class="edu.wustl.geneconnect.domain.impl.GenomicIdentifierSetImpl" />
    </set>
    <set name="consensusIdentifierDataCollection" lazy="true" inverse="true">
      <cache usage="read-write" />
      <key column="SOLUTION_ID" />
      <one-to-many class="edu.wustl.geneconnect.domain.impl.ConsensusIdentifierDataImpl" />
    </set>
  </class>
</hibernate-mapping>

